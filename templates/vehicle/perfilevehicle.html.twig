{# empty Twig template #}
{% extends 'base.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block body %}
    {% if app.user %}
       {% for vehicle in vehicles %}
        <div class="infoperfil userdata">
            <div class="infoperfiltitol"><h2>Vehiculo</h2>
            </div>
                <h3>Marca: <i>{{ vehicle.marca }}</i></h3>
                <h3>Km: <i>{{ vehicle.Km }}</i></h3>
                <h3>Color: <i>{{ vehicle.color }}</i></h3>
                <h3>Puertas: <i>{{ vehicle.puertas }}</i></h3>
                <h3>Plazas: <i>{{ vehicle.plazas }}</i></h3>
                <h3>Cambio: <i>{{ vehicle.Cambio }}</i></h3>
                <h3>Combustible: <i>{{ vehicle.Combustible}}</i></h3>
                <h3>Price: <i>{{ vehicle.price }}</i></h3>
                 <div class="compraveh">
                   <a href="{{path("vehiculo")}}">Ver Vehiculo</a>
                  </div>

            </div>
          {%endfor%}
    {%endif%}
{% endblock %}
